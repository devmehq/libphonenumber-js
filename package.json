    "rollup": "^4.20.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.2.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
